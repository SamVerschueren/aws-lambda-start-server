{
  "name": "aws-lambda-start-server",
  "version": "0.1.2",
  "description": "AWS Lambda function that starts a server",
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "github.com/SamVerschueren"
  },
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "npm run lint && nyc ava dist/test",
    "pretest": "npm run clean && npm run compile -- --sourceMap",
    "compile": "tsc -p tsconfig.json",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "npm run clean && npm run compile && npm run build.prepare && npm run build.zip",
    "build.prepare": "del-cli dist/test && cpy package.json dist && cd dist && npm i --prod && cd ..",
    "build.zip": "cd dist && zip -r ../build.zip . && cd ..",
    "clean": "del-cli .nyc_output coverage dist build.zip",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "aws",
    "lambda",
    "start",
    "server",
    "cronjob"
  ],
  "dependencies": {
    "bragg": "^2.1.2",
    "bragg-cron": "^1.0.0",
    "bragg-router": "^2.1.0",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.8",
    "ava": "*",
    "aws-lambda-pify": "^3.0.0",
    "aws-sdk": "^2.117.0",
    "aws-sdk-mock": "^1.7.0",
    "coveralls": "^2.13.1",
    "cpy-cli": "^1.0.1",
    "del-cli": "^0.2.1",
    "nyc": "^10.1.2",
    "sinon": "^3.2.1",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2"
  },
  "nyc": {
    "exclude": [
      "dist/test"
    ]
  }
}
